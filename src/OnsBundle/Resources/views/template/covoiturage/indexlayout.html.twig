<script>
    function chercherCle(){
        var x=document.getElementById("cle").value;
        var xhr = new XMLHttpRequest();
        xhr.open("GET","chercherCleCovoiturage.html?cle="+x,true);
        xhr.send(null);
        xhr.onreadystatechange = function result(){
            if((xhr.status == 200)&&(xhr.readyState==4))
                document.getElementById("chercheCle").innerHTML= xhr.responseText;
        }
    }

    function consulter(id,idU){
        var x=id;
        var y=idU;
        var xhr = new XMLHttpRequest();
        xhr.open("GET","consulterCovoiturageA.html?idCov="+x+"&idConducteur="+y,true);
        xhr.send(null);
        xhr.onreadystatechange = function result(){
            if((xhr.status == 200)&&(xhr.readyState==4))
                document.getElementById("covoiturageDetails").innerHTML= xhr.responseText;
        }
    }
    function reserver(id,idU) {
        var x = id;
        var y=idU;
        var xhr = new XMLHttpRequest();
        xhr.open("GET", "reserverCovoiturage.html?idCov="+x+"&idConducteur="+y, true);
        xhr.send(null);
        xhr.onreadystatechange = function result() {
            if ((xhr.status == 200) && (xhr.readyState == 4))
                document.getElementById("chercheCle").innerHTML = xhr.responseText;
        }
    }
</script>



